<!-- STATES INDEX PAGE -->

<div class="container push_page_below_header">
  <div class="row">
    <h1>National </h1>

    <h2>Provinces</h2>
  </div>

  <div class="table">
    <% @states.each do |s| %>
      <ul class='container table'>

        <li><%= link_to(s.state_name, state_path(s)) %></li>
        <li><%= 'Population Report' %></li>
        <li><%= link_to("Stock Report", state_stock_reports_path(s)) %></li>
        <li><%= 'Program Report' %></li>
        <span class="stretch"></span>

      </ul>

    <% end %>
  </div>
</div>

